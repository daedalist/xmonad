! special
*.foreground:   #f4d890
*.background:   #1f1f1f
*.cursorColor:  #ffc500

! black
*.color0:       #c0b18b
*.color8:       #c0b18b

! red
*.color1:       #ff7322
*.color9:       #ff7322

! green
*.color2:       #ffc500
*.color10:      #ffc500

! yellow
*.color3:       #d47c1d
*.color11:      #d47c1d

! blue
*.color4:       #ffcf01
*.color12:      #ffcf01

! magenta
*.color5:       #ffbe00
*.color13:      #ffbe00

! cyan
*.color6:       #fdcd56
*.color14:      #fdcd56

! white
*.color7:       #fbd277
*.color15:      #fbd277

Xft.dpi: 90
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

URxvt.saveLines:        10000
URxvt.xftAntialias:     true
URxvt.font:             xft:inconsolata:size=12:antialias=true:hinting=true
URxvt.boldFont:         xft:inconsolata:size=12:antialias=true:hinting=true
URxvt.italicFont:       xft:inconsolata:size=12:antialias=true:hinting=true
URxvt.bolditalicFont:   xft:inconsolata:size=12:antialias=true:hinting=true
urxvt*scrollBar: false
URxvt.internalBorder:40
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: google-chrome-stable
URxvt.matcher.button: 1
URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0
URxvt.letterSpace: -2
! vim: ft=xdefaults
